{
    "name": "spatie/nova-tail-tool",
    "description": "A Laravel Nova tool to display the application log.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../nova"
        }
    ],
    "homepage": "https://github.com/spatie/nova-tail-tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*",
        "orchestra/testbench": "^3.6",
        "spatie/phpunit-snapshot-assertions": "^1.3",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\NovaTailTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\NovaTailTool\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\NovaTailTool\\NovaTailToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
